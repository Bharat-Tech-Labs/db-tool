{
  "name": "db-query-tool",
  "productName": "db-query-tool",
  "version": "0.4.1",
  "private": true,
  "description": "updated description",
  "scripts": {
    "serve": "yarn --cwd ./API build && yarn --cwd ./DashBoard electron"
  },
  "dependencies": {
    "@commitlint/cli": "^9.1.1",
    "@commitlint/config-angular": "^9.1.1",
    "@commitlint/config-conventional": "^9.1.1",
    "@commitlint/config-lerna-scopes": "^9.1.1",
    "commitizen": "^4.1.2",
    "electron-log": "^4.2.2",
    "lerna": "^3.22.1",
    "less-loader": "^6.2.0",
    "lint-staged": ">=10",
    "readline-sync": "^1.4.10"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "@vue/eslint-config-typescript": "^5.0.2",
    "cz-conventional-changelog": "3.2.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "husky": ">=4",
    "sass": "^1.19.0",
    "sass-loader": "^8.0.0",
    "typescript": "~3.9.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true",
      "commit-msg": "node --experimental-modules scripts/commitMessageHook.mjs && commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix"
  },
  "workspaces": [
    "DashBoard",
    "API"
  ]
}
